<script>
    function showInputToChangePriority(id, priority, bool){
        if(bool){
            $(`#priorityTD${id}`).html(`
                <input type="number" class="form-control" id="input${id}" value="${priority}" min="1" style="width:100px;">
                <button type="button" id="btn${id}" class="btn btn-primary btn-sm mx-2" onClick="changePriority('${id}', '${priority}')">change</button>

            `)
        }else{
            $(`#priorityTD${id}`).html(`${priority}`)
        }
    }


    function checkUncheckRowCheckBox(id, priority){
        console.log(id)
        let isCheck = $(`#exampleCheck${id}`).is(":checked");
        console.log(isCheck)
        $("#myTable tbody tr").each(function(){
            let rowCheckId = $(this).find("input[class='form-check-input']").attr('id');
            if(isCheck){
                if(rowCheckId != `exampleCheck${id}`){
                    $(`#${rowCheckId}`).attr("disabled", true);
                }
            }else{
                if(rowCheckId != `exampleCheck${id}`){
                    $(`#${rowCheckId}`).removeAttr("disabled");
                }
            }
            
        });
        showInputToChangePriority(id, priority, isCheck)
    }

    function changePriority(rowId, oldPriority){
        let changePriority = $(`#input${rowId}`).val();
        $(`#btn${rowId}`).html('<i class="fa fa-spinner fa-spin"></i>Loading');
        if(changePriority != ''){
            let reqObj = {id : rowId, originalPriority : oldPriority, changePriority : changePriority};
            var urls = "http://nodeserver.mydevfactory.com:8082/api/v1/ebook/update-priority/";
    
            $.ajax({
                type: "POST",
                url: urls,
                data: JSON.stringify(reqObj),
                headers: {
                    Authorization: `Bearer ${localStorage.getItem('token')}`,
                },
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                error: function() {
                    alert("Error");
                },
                success: function(response) {
                    $(`#btn${rowId}`).html('change');
                    Swal.fire({
                        icon : response.status,
                        title: response.message,
                        allowOutsideClick: false,
                        confirmButtonText: 'Click',
                    }).then((result) => {
                        if (result.isConfirmed) {
                            window.location.reload();
                        }
                    })
                }
            });
        
        }else{
            $(`#btn${rowId}`).html('change');
            $(`#errorMessage`).fadeIn('slow').html('Priority input field is required.');
            setTimeout(function(){
                $(`#errorMessage`).fadeOut('slow').html('');
            },3000)
        }
    }
</script>

